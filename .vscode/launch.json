    "version": "0.2.0",
    "configurations": [
        {
            "name": "Thrift Client",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/thrift",
            "env": {
                "HTTP_PROXY":"localhost:9090"
            },
            "args": []
        },
        {
            "name": "Thrift Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/thrift",
            "env": {
                "HTTP_PROXY":"localhost:9090"
            },
            "args": [
                "-server"
            ]
        }
    ]
}